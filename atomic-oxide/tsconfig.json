{
	"compilerOptions": {
		"noEmit": true,
		"module": "es2022",
		"target": "es2021",
		"lib": [
			"es2021",
			"DOM"
		],
		"esModuleInterop": true,
		"strict": true,
		"alwaysStrict": true,
		"preserveConstEnums": true,
		"moduleResolution": "node",
		"sourceMap": true,
		"types": [],
		"resolveJsonModule": true,
		"allowJs": true,
		"checkJs": false,
		"skipLibCheck": true
	},
	"types": [
		"node"
	],
	"include": [
		"client",
		"types.d.ts"
	],
	"exclude": [
		"node_modules",
		"dist",
		"test"
	]
}